{
  "name": "@mayara/signalk-radar",
  "version": "0.1.3",
  "description": "MaYaRa Radar - Multi-brand marine radar integration for SignalK",
  "scripts": {
    "build": "node build.js",
    "build:test": "node build.js --test",
    "pack": "node build.js --pack",
    "test": "cargo test -p mayara-core"
  },
  "wasmManifest": "plugin.wasm",
  "wasmCapabilities": {
    "rawSockets": true,
    "dataWrite": true,
    "radarProvider": true
  },
  "signalk": {
    "displayName": "MaYaRa Radar",
    "appIcon": "assets/mayara_logo.png",
    "appstore": {
      "displayName": "MaYaRa Radar",
      "categories": [
        "utilities"
      ],
      "description": "MaYaRa Radar - Multi-brand marine radar integration for SignalK"
    },
    "webapp": {
      "name": "MaYaRa Radar",
      "description": "MaYaRa Radar - Multi-brand marine radar integration for SignalK",
      "location": "/plugins/mayara-radar/"
    }
  },
  "keywords": [
    "signalk-node-server-plugin",
    "signalk-wasm-plugin",
    "signalk-webapp",
    "radar",
    "marine",
    "furuno",
    "navico",
    "raymarine",
    "garmin"
  ],
  "author": "Mayara Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mayara/mayara"
  },
  "engines": {
    "signalk": ">=2.0.0"
  },
  "files": [
    "plugin.wasm",
    "public/**/*"
  ]
}
